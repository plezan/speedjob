{
  "plugins": [
    "./node_modules/jsdoc-vuejs",
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "src/",
      "README.md"
    ],
    "includePattern": "\\.(vue|js)$"
  },
  "recurseDepth": 10,
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "recurse": true
  },
  "jsdoc-vuejs": {
    "followImports": false
  }
}
